/* pages/login/login.wxss */
.login{
  padding: 20rpx;
  box-sizing: border-box;
}
.login image{
  width: 100%;
  height: 730rpx;
  display: block;
}
.loginBtn{
  background-color: #fff;
  text-align: center;
  padding: 0 50rpx 70rpx;
}
.loginBtn navigator{
  padding: 20rpx 0;
  color: #fff;
  background-color: #31ac3a;
  border-radius: 200rpx;
}
.read{
  display: flex;
  font-size: 24rpx;
  align-items: center;
  position: fixed;
  bottom: 80rpx;
  left: 20rpx;
}
.read text{
  margin-right: 10rpx;
}

.read navigator{
  color: #0029a7;
}


